# === OpenAI ===
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL_NAME=gpt-4
OPENAI_TEMPERATURE=0
OPENAI_MAX_TOKENS=2048

# === Tavily ===
TAVILY_API_KEY=your_tavily_api_key

# === xAI Grok ===
GROK_API_KEY=your_grok_api_key

# === Email ===
EMAIL_PASSWORD=your_email_app_password

# === MongoDB ===
MONGO_URI=mongodb://admin:admin123@localhost:27017/
MONGODB_DB_NAME=resume_jd_db

# === AWS S3 ===
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=ap-south-1
S3_BUCKET_NAME=your-resume-bucket-name
S3_PREFIX=resumes/

# === PostgreSQL ===
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=resume_jd_db

# === MySQL ===
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=your_mysql_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DB=resume_jd_db

# === Google Drive (OAuth2) ===
GDRIVE_CLIENT_ID=your_google_drive_client_id
GDRIVE_CLIENT_SECRET=your_google_drive_client_secret
GDRIVE_REFRESH_TOKEN=your_refresh_token
GDRIVE_TOKEN_URI=https://oauth2.googleapis.com/token
GDRIVE_FOLDER_ID=your_drive_folder_id_for_resumes

# === General Config ===
CONNECTOR_TYPE=mongo   # Options: mongo, s3, postgres, mysql, gdrive
